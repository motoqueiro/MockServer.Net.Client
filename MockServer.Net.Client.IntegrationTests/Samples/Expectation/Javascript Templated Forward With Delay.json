{
 "httpRequest": {
 "path": "/some/path"
 },
 "httpForwardTemplate": {
 "template": "return {\n" +
 " 'path' : \"/somePath\",\n" +
 " 'cookies' : {\n" +
 " 'SessionId' : request.cookies && request.cookies['SessionId']\n" +
 " },\n" +
 " 'headers' : {\n" +
 " 'Host' : [ \"localhost:1081\" ]\n" +
 " },\n" +
 " 'keepAlive' : true,\n" +
 " 'secure' : true,\n" +
 " 'body' : \"some_body\"\n" +
 "};",
 "templateType": "JAVASCRIPT",
 "delay": {"timeUnit": "SECONDS", "value": 20}
 }
}